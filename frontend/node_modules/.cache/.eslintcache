ber.parseFloat(Number(cart.itemPrice) + Number(cart.shippingPrice) + Number(cart.taxPrice)).toFixed(2);\r\n\r\n    const addOrder = {\r\n        orderItems: cartItems,\r\n        shippingAddress: shippingAddress,\r\n        payment: payment.payment,\r\n        itemPrice: cart.itemPrice,\r\n        shippingPrice: cart.shippingPrice,\r\n        taxPrice: cart.taxPrice,\r\n        totalPrice: cart.totalPrice,\r\n    }\r\n\r\n    const orderCreate = useSelector(state => state.reducer.orderCreate);\r\n    const { order, success, error } = orderCreate;\r\n\r\n    const placeOrderHandler = () => {\r\n        dispatch(createOrder(addOrder))\r\n    }\r\n\r\n    useEffect(() => {\r\n        if(success){\r\n            navigate(`/order/${order._id}`);\r\n        }\r\n    }, [navigate, success])\r\n\r\n  return (\r\n    <Container>\r\n        <CheckoutProcess step1 step2 step3/>\r\n        <Row>\r\n            <Col md={8}>\r\n                <ListGroup variant='flush'>\r\n                    {/* Shipping address */}\r\n                    <ListGroup.Item className='list--group'>\r\n                        <h2>Shipping</h2>\r\n                        <p>\r\n                            <strong>Address: </strong>\r\n                            {shippingAddress.address}, {shippingAddress.city}, {shippingAddress.postalCode}, {shippingAddress.country}\r\n                        </p>\r\n                    </ListGroup.Item>\r\n                    {/* Payment Method */}\r\n                    <ListGroup.Item className='list--group'>\r\n                        <h2>Payment Method</h2>\r\n                            <strong>Choice of payment: </strong>\r\n                            {payment.payment}\r\n                    </ListGroup.Item>\r\n                    {/* Items ordered */}\r\n                    <ListGroup.Item className='list--group'>\r\n                        <h2>Your Order</h2>\r\n                        <ListGroup variant='flush'>\r\n                            {cartItems.map(item => \r\n                                <ListGroup.Item key={item.product} className='cart--items'>\r\n                                <Row>\r\n                                  <Col md={2}>\r\n                                    <Image src={item.image} fluid rounded />\r\n                                  </Col>\r\n                                  <Col md={4}>\r\n                                    {item.name}\r\n                                  </Col>\r\n                                  <Col md={4}>\r\n                                    ${item.price}\r\n                                  </Col>\r\n                                  <Col md={2}>\r\n                                    {item.quantity}\r\n                                  </Col>\r\n                                </Row>\r\n                              </ListGroup.Item>    \r\n                            )}\r\n                        </ListGroup>\r\n                    </ListGroup.Item>\r\n                </ListGroup>\r\n            </Col>\r\n            {/* Order summary */}\r\n            <Col md={4} className='list--group'>\r\n                <Card className='payment--checkout--card'>\r\n                    <ListGroup variant='flush'>\r\n                        <ListGroup.Item>\r\n                            <h2>Order Summary</h2>\r\n                        </ListGroup.Item>\r\n                        <ListGroup.Item>\r\n                            <Row>\r\n                                <Col>Total Items:</Col>\r\n                                <Col>${cart.itemPrice}</Col>\r\n                            </Row>\r\n                        </ListGroup.Item>\r\n                        <ListGroup.Item>\r\n                            <Row>     \r\n                                <Col>Shipping:</Col>\r\n                                <Col>${cart.shippingPrice}</Col>\r\n                            </Row>\r\n                        </ListGroup.Item>\r\n                        <ListGroup.Item>\r\n                            <Row>    \r\n                                <Col>Tax:</Col>\r\n                                <Col>${cart.taxPrice}</Col>\r\n                            </Row>\r\n                        </ListGroup.Item>\r\n                        <ListGroup.Item>\r\n                            <Row>\r\n                                <Col>Total:</Col>\r\n                                <Col>${cart.totalPrice}</Col>\r\n                            </Row>\r\n                        </ListGroup.Item>\r\n                        <ListGroup.Item>\r\n                            {error && <Message variant='danger'>{error}</Message>}\r\n                        </ListGroup.Item>\r\n                        <ListGroup.Item className='list--group'>\r\n                            <Button type='button' className='rounded my-3' style={{width: \"100%\"}} variant='dark' onClick={placeOrderHandler}>Finish Order</Button>\r\n                        </ListGroup.Item>\r\n                    </ListGroup>\r\n                </Card>\r\n            </Col>\r\n        </Row>\r\n    </Container>\r\n  )\r\n}\r\n\r\nexport default PlaceOrderScreen",[],"C:\\Users\\Noah\\Desktop\\Portfolio\\merchandise-marketplace\\frontend\\src\\screens\\OrderScreen.jsx",[],[],"C:\\Users\\Noah\\Desktop\\Portfolio\\merchandise-marketplace\\frontend\\src\\constants\\userConstants.js",[],[],"C:\\Users\\Noah\\Desktop\\Portfolio\\merchandise-marketplace\\frontend\\src\\constants\\orderConstants.js",[],[],"C:\\Users\\Noah\\Desktop\\Portfolio\\merchandise-marketplace\\frontend\\src\\constants\\productConstants.js",[],[],"C:\\Users\\Noah\\Desktop\\Portfolio\\merchandise-marketplace\\frontend\\src\\components\\Search.jsx",[],[],"C:\\Users\\Noah\\Desktop\\Portfolio\\merchandise-marketplace\\frontend\\src\\actions\\cartActions.js",[],[],"C:\\Users\\Noah\\Desktop\\Portfolio\\merchandise-marketplace\\frontend\\src\\components\\Product.jsx",[],[],"C:\\Users\\Noah\\Desktop\\Portfolio\\merchandise-marketplace\\fro